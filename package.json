{
  "name": "accumulus",
  "version": "1.0.0",
  "description": "A visualization and optimization insight tool for AWS Lambda",
  "main": "/src/index.js/",
  "type": "module",
  "author": "Eric Wu, Matt Femia, Dan Teng, Christian Sansovich, Mark Yencheske",
  "scripts": {
    "clean": "rm dist/bundle.js",
    "compile-dev": "npx tsc -w",
    "compile-prod": "tsc",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start": "webpack serve nodemon ./dist/server/server.js",
    "dev": "NODE_ENV=development webpack-dev-server --open & nodemon --es-module-specifier-resolution=node dist/server/server.js",
    "shep-build": "webpack --watch --mode development",
    "shep-dev": "NODE_ENV=development webpack server --open & nodemon server/server.js",
    "chal-start": "nodemon ./server/server.ts & webpack-dev-server",
    "test": "jest",
    "build": "tsc -p tsconfig.build.json",
    "start-docker-webpack": "NODE_ENV=production REACT_APP_DOMAIN_PROXY=https://www.accumulus.dev webpack serve",
    "start-docker-app": "NODE_ENV=production DOMAIN_PROXY=https://www.accumulus.dev node --es-module-specifier-resolution=node ./dist/server/server.js",
    "build-docker-webpack": "docker build -f webpack.Dockerfile . -t webpack",
    "build-docker-app": "docker build -f app.Dockerfile . -t app",
    "push-docker-webpack": "aws ecr get-login-password --region us-east-2 --profile accumulus | docker login --username AWS --password-stdin 070809985305.dkr.ecr.us-east-2.amazonaws.com & docker build -f webpack.Dockerfile -t accumulus-webpack . & docker tag accumulus-frontend:latest 070809985305.dkr.ecr.us-east-2.amazonaws.com/accumulus-frontend:latest & docker push 070809985305.dkr.ecr.us-east-2.amazonaws.com/accumulus-webpack:latest",
    "push-docker-app": "aws ecr get-login-password --region us-east-2 --profile accumulus | docker login --username AWS --password-stdin 070809985305.dkr.ecr.us-east-2.amazonaws.com & docker build -f app.Dockerfile -t accumulus-app . & docker tag accumulus-backend:latest 070809985305.dkr.ecr.us-east-2.amazonaws.com/accumulus-frontend:latest & docker push 070809985305.dkr.ecr.us-east-2.amazonaws.com/accumulus-app:latest",
    "production": "NODE_ENV=development nodemon ./server/server.js & webpack-dev-server --open --hot",
    "dev:hot": "NODE_ENV=development nodemon --es-module-specifier-resolution=node ./dist/server/server.js & webpack-dev-server --open --mode development",
    "docker-dev:hot": "docker-compose -f docker-compose-dev.yml up",
    "accumulus": "NODE_ENV=production node --es-module-specifier-resolution=node ./dist/server/server.js"
  },
  "keywords": [],
  "license": "ISC",
  "resolutions": {
    "styled-components": "^5"
  },
  "babel": {
    "plugins": [
      "babel-plugin-styled-components"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.78.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.81.0",
    "@aws-sdk/client-lambda": "^3.78.0",
    "@aws-sdk/client-sts": "^3.78.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@react-three/drei": "^9.11.0",
    "@react-three/fiber": "^8.0.17",
    "@tidyjs/tidy": "^2.4.5",
    "acorn": "^8.7.1",
    "aws-sdk": "^2.1122.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "ioredis": "^5.0.4",
    "lamina": "^1.1.19",
    "moment": "^2.29.3",
    "mongoose": "^6.3.1",
    "perlin": "^1.0.0",
    "postcss": "^8.4.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.30.0",
    "react-router-dom": "^5.3.0",
    "recharts": "^2.1.9",
    "sass": "^1.51.0",
    "styled-components": "^5.3.5",
    "three": "^0.140.2",
    "three-custom-shader-material": "^3.3.6",
    "three-noise": "^1.1.2",
    "supertest": "^6.2.3",
    "uuid": "^8.3.2",
    "webpack": "^5.72.0",
    "webpack-hot-middleware": "^2.25.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.23",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/styled-components": "^5.1.25",
    "@types/three": "^0.140.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "babelify": "^10.0.0",
    "css-loader": "^6.7.1",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "sass-loader": "^12.6.0",
    "source-map-loader": "^1.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
